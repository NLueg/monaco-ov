{
  "name": "monaco-ov",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run compile && webpack && npm run copy",
    "clean": "rimraf dist",
    "compile": "tsc",
    "copy": "ncp src/index.html dist/index.html",
    "prepare": "npm run clean && npm run build",
    "start": "npm run build && node dist/server.js",
    "start:watch": "nodemon",
    "test": "npm run build && testcafe",
    "watch": "tsc -w"
  },
  "dependencies": {
    "monaco-editor": "^0.19.1",
    "monaco-languageclient": "0.12.0",
    "normalize-url": "^5.0.0",
    "vscode-ws-jsonrpc": "^0.2.0",
    "ov-language-server": "^0.1.6",
    "ov-language-server-types": "^0.1.2",
    "reconnecting-websocket": "^4.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.3",
    "@types/ws": "^7.2.3",
    "css-loader": "^3.4.2",
    "express": "^4.18.2",
    "file-loader": "^5.1.0",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.2.5",
    "testcafe": "^1.8.3",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
